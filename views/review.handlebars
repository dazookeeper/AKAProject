<!DOCTYPE html>
<html>
<head>
	<title>Music AKA | Life's an Entertainment</title>
	{{> scripts}}
	<script src="../js/review.js"></script>
</head>
<body>
{{> nav}}

<!-- banner -->
		<div id="me" class="panel">
			<center>
		    <font id="biggreen">MUSIC</font> 
		    <font id="biggray">REVIEW</font>
		  </center>

		<!-- contact -->
		<div class="contact-top" style="padding:0px 12px;" >
				<div class="contact-form">
					<form>
						<input id="who" type="text" placeholder="Who did you see?" required="">
						<input id="where" type="text" placeholder="Where did you see them?" required="">
		        	
		        	<center>
		        	<div class="col-sm-9">
		            <div class="btn-group" data-toggle="buttons" required="">
	                <label class="btn btn-default">
	                    <input type="radio" id="q156" name="quality" value="1" /> 1 
	                </label> 
	                <label class="btn btn-default">
	                    <input type="radio" id="q157" name="quality" value="2" /> 2
	                </label> 
	                <label class="btn btn-default">
	                    <input type="radio" id="q158" name="quality" value="3" /> 3
	                </label> 
	                <label class="btn btn-default">
	                    <input type="radio" id="q159" name="quality" value="4" /> 4
	                </label> 
	                <label class="btn btn-default">
	                	<input type="radio" id="q159" name="quality" value="5"/> 5
	                </label> 
		            </div>
		          </div>
		          </center>
		          <br>
		          <center>
		        	<div class="col-sm-9">
		            <div class="btn-group" data-toggle="buttons" required="">
	                <label class="btn btn-default">
	                    <input type="radio" id="q128" name="mood" value="1" checked="checked" /> <img src="http://4.bp.blogspot.com/-ZgtYQpXq0Yo/UZEDl_PJLhI/AAAAAAAADnk/2pgkDG-nlGs/s1600/facebook-smiley-face-for-comments.png"> 
	                </label> 
	                <label class="btn btn-default">
	                    <input type="radio" id="q129" name="mood" value="2" /> <img src="http://3.bp.blogspot.com/-2xtlB3F7l4U/UZFOZ1FWvrI/AAAAAAAADoI/9P9JWS-ZIEI/s1600/facebook-cry-emoticon-crying-symbol.png">
	                </label> 
	                <label class="btn btn-default">
	                    <input type="radio" id="q130" name="mood" value="3" /> <img src="http://1.bp.blogspot.com/-MB8OSM9zcmM/TvitChHcRRI/AAAAAAAAAiE/kdA6RbnbzFU/s400/surprised%2Bemoticon.png">
	                </label> 
	                <label class="btn btn-default">
	                    <input type="radio" id="q131" name="mood" value="4" /> <img src="http://3.bp.blogspot.com/-wn2wPLAukW8/U1vy7Ol5aEI/AAAAAAAAGq0/f7C6-otIDY0/s1600/squinting-emoticon.png">
	                </label> 
	                <label class="btn btn-default">
	                	<input type="radio" id="q132" name="mood" value="5" /> <img src="http://3.bp.blogspot.com/-h2ErlFczszQ/TvirzZanEUI/AAAAAAAAAh4/rghgvQxSXmQ/s400/devil%2Bemoticon.png">
	                </label> 
		            </div>
		          </div>
		          </center>

		    		<br>
						<input type="text" placeholder="Describe your experience in one sentence!" required="" id="summary">
						<textarea placeholder="Your chance to elaborate..." required="" id="review"></textarea>
						
						<span class="btn btn-default btn-file">
						  Browse to upload pictures... <input type="file" id="file_input">
						</span>

						<br>
						<br>
						
						<input type="submit" class="form-control" value="submit" id="submitreview">
            <input type="hidden" id="review_url" name="review_url" value="https://upload.wikimedia.org/wikipedia/commons/thumb/a/ac/No_image_available.svg/300px-No_image_available.svg.png" />

            <br>
					</form>
				</div>
				</div>
				</div>
			<div class="clearfix"></div>
		</div>
</body>
</html>